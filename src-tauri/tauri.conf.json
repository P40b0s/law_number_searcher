{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "number_seracher",
  "version": "0.1.5",
  "identifier": "ru.n-searcher.app",
  "build": {
    "devUrl": "http://localhost:8080",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Поиск пропущенных номеров",
        "width": 635,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
